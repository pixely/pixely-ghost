{
  "name": "pixely-ghost",
  "description": "Ghost theme for personal site of Graham Martin",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Graham Martin",
  "homepage": "https://docs.ghost.org/api/gatsby/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixely/pixely-ghost.git"
  },
  "engines": {
    "node": ">= 10.13.0"
  },
  "bugs": {
    "url": "https://github.com/pixely/pixely-ghost.git/issues"
  },
  "keywords": [
    "gatsby",
    "ghost"
  ],
  "main": "n/a",
  "scripts": {
