{
  "name": "pixely-ghost",
  "description": "Ghost theme for personal site of Graham Martin",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Graham Martin",
  "homepage": "https://github.com/pixely/pixely-ghost",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixely/pixely-ghost.git"
  },
  "engines": {
    "node": ">= 10.13.0"
  },
  "bugs": {
    "url": "https://github.com/pixely/pixely-ghost.git/issues"
  },
  "keywords": [
    "ghost"
  ],
  "main": "n/a",
  "scripts": {
    "serve": "yarn build && NODE_ENV=production gatsby serve",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build",
    "dev": "gatsby develop -H 0.0.0.0",
    "lint": "yarn lint:es; yarn lint:stylelint",
    "lint:es": "eslint . --ext .js --cache",
    "lint:stylelint": "stylelint \"**/**.scss\"",
    "test": "yarn lint",
    "11ty:dev": "cross-env ELEVENTY_ENV=dev concurrently \"eleventy --serve\" \"yarn assets:watch\"",
    "11ty:build": "cross-env ELEVENTY_ENV=prod eleventy && yarn assets:build",
    "assets:watch": "parcel watch src/**/*.scss src/js/*.js",
    "assets:build": "parcel build src/**/*.scss src/js/*.js"
  },
  "devDependencies": {
    "@danbruegge/gatsby-plugin-stylelint": "^4.1.0",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.3.0",
    "eslint": "7.16.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-ghost": "2.0.0",
    "eslint-plugin-react": "7.21.5",
    "gatsby-plugin-eslint": "^2.0.8",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.30.0",
    "stylelint": "^13.8.0",
    "stylelint-config-airbnb": "^0.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@tryghost/content-api": "^1.4.11",
    "@tryghost/helpers": "^1.1.35",
    "@tryghost/helpers-gatsby": "1.0.40",
    "cheerio": "1.0.0-rc.5",
    "classnames": "^2.2.6",
    "clean-css": "^4.2.3",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eleventy-plugin-lazyimages": "^2.1.0",
    "eleventy-plugin-local-images": "^0.4.0",
    "eleventy-plugin-sass": "^1.1.1",
    "eslint-utils": "^2.1.0",
    "gatsby": "2.27.0",
    "gatsby-awesome-pagination": "0.3.6",
    "gatsby-plugin-advanced-sitemap": "1.5.6",
    "gatsby-plugin-extract-image-colors": "^0.0.14",
    "gatsby-plugin-feed": "2.8.0",
    "gatsby-plugin-force-trailing-slashes": "1.0.4",
    "gatsby-plugin-ghost-images": "^1.5.2",
    "gatsby-plugin-manifest": "2.7.0",
    "gatsby-plugin-offline": "3.5.0",
    "gatsby-plugin-react-helmet": "3.5.0",
    "gatsby-plugin-sharp": "^2.6.27",
    "gatsby-rehype-ghost-links": "^1.5.2",
    "gatsby-rehype-inline-images": "^1.9.2",
    "gatsby-source-filesystem": "2.5.0",
    "gatsby-transformer-rehype": "^1.8.1",
    "gatsby-transformer-sharp": "^2.5.13",
    "html-minifier": "^4.0.0",
    "jamify-source-ghost": "^1.16.1",
    "lodash": "4.17.20",
    "node-sass": "^4.13.1",
    "normalize.css": "^8.0.1",
    "nunjucks": "^3.2.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "rehype": "^11.0.0",
    "rehype-format": "^3.1.0",
    "rehype-partials": "^1.0.6",
    "rehype-react": "^6.1.0"
  }
}
