<script>
	const animationElement = document.querySelector('.js-animate-diagram');
    const options = { threshold: 0.1 }
    const startAnimate = (entries) => { entries.forEach(entry => { entry.isIntersecting && entry.target.classList.add('is-animated')})};
    const observer = new IntersectionObserver(startAnimate, options);
    observer.observe(animationElement);
</script>
